<view id="all-apply-list"><cu-custom vue-id="1" bgColor="bg-informatic-brown" isBack bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">所有申请</view></cu-custom><view class="cu-bar bg-white solids-bottom"><view class="action"><text class="cuIcon-title text-blue text-xxl"></text><text class="text-xxl text-bold">实验室申请</text></view><view data-event-opts="{{[['tap',[['foldUp',['$event']]]]]}}" class="action" bindtap="__e"><text class="text-df">{{display?'收起':'展开'}}</text><text class="{{['padding-lr-xs',display?'cuIcon-fold':'cuIcon-unfold']}}"></text></view></view><transition-group class="cu-list cu-card" vue-id="2" name="list" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toExecute',['$0'],[[['data','',index]]]]]]]}}" hidden="{{!(display)}}" class="cu-item bg-informatic-brown shadow" bindtap="__e"><sticky vue-id="{{('3-'+index)+','+('2')}}" item="{{item}}" bind:__l="__l"></sticky></view></block></transition-group><block wx:if="{{data.length===0&&display}}"><view class="padding-tb text-center text-lg"><text class="text-bold text-gray">暂无数据</text></view></block></view>