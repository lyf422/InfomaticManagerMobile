
.cu-card>.cu-item {
	-webkit-transition: all 1s;
	transition: all 1s;
}
.list-move{
	-webkit-transition: all 0.8s;
	transition: all 0.8s;
}
.list-enter{
	opacity: 0;
	-webkit-transform: translateY(-30px);
	        transform: translateY(-30px);
}
.list-leave-to{
	opacity: 0;
	-webkit-transform: translateY(-30px);
	        transform: translateY(-30px);
}


