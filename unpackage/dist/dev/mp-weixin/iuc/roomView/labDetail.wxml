<view id="lab-detail"><cu-custom vue-id="1" bgColor="bg-informatic-brown" isBack bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">实验室详细信息</view></cu-custom><lab-info-card class="margin-lr-xl" vue-id="2" lab="{{labInfo}}" bind:__l="__l"></lab-info-card><block wx:if="{{labInfo.RoomType===10}}"><view class="text-center"><block wx:if="{{labInfo.RoomDisplayState!=='待填写'}}"><view><button data-event-opts="{{[['tap',[['submit',['$0','按团队申请实验室'],['labInfo.ID']]]]]}}" class="cu-btn bg-blue lg margin" bindtap="__e">以团队申请该实验室</button></view></block><block wx:else><view><button class="cu-btn bg-blue lg" disabled type>{{labInfo.RoomDisplayState}}</button><view class="_br"></view><block wx:if="{{$root.g0}}"><button class="cu-btn bg-blue lg margin-top" type data-event-opts="{{[['tap',[['releaseRoom',['$0'],['labInfo.ID']]]]]}}" bindtap="__e">强制释放</button></block></view></block></view></block><block wx:else><block wx:if="{{labInfo.RoomType===20}}"><view><view class="action text-xl bg-white solids-bottom padding-sm"><text class="cuIcon-title text-blue text-xl"></text><text class="text-bold text-xl">机位列表</text></view><block wx:if="{{applicationData.length>0}}"><view class="cu-list menu"><block wx:for="{{applicationData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item"><view class="content"><text class="cuIcon-btn text-olive"></text><text class="text-grey">{{'机位'+(item.Code+'\t')+"配置："+item.Memo+''}}</text></view><view class="action"><button class="cu-btn round bg-green shadow" disabled="{{item.State!==0}}" data-event-opts="{{[['tap',[['submit',['$0'],[[['applicationData','',index,'ID']]]]]]]}}" bindtap="__e"><text class="cuIcon-upload"></text>申请</button></view><view class="action"><block wx:if="{{$root.g1}}"><button class="cu-btn round bg-green shadow" disabled="{{item.State===0}}" data-event-opts="{{[['tap',[['release',['$0'],[[['applicationData','',index,'ID']]]]]]]}}" bindtap="__e"><text class="cuIcon-upload"></text>释放</button></block></view></view></block></view></block><block wx:else><view class="text-center padding-top-xl"><text>暂无内容</text></view></block></view></block><block wx:else><view class="margin-lr-xl padding-lr-xl"><image mode="aspectFit" src="../../static/无需申请.png"></image></view></block></block><nav-tab vue-id="3" selection="{{0}}" bind:__l="__l"></nav-tab></view>